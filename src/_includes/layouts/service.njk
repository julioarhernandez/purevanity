    <div class="page-wrapper">
      <header class="main-header">
        <div class="main-header__top">
          <div class="main-header__top-wrapper">
            <div class="main-header__top-inner">
              <div class="main-header__top-left">
                <ul class="list-unstyled main-header__contact-list">
                  <li>
                    <div class="icon">
                      <i class="fas fa-envelope"></i>
                    </div>
                    <div class="text">
                      <p>
                        <a href="{{ site.email }}">{{ site.email }}</a>
                      </p>
                    </div>
                  </li>
                  <li>
                    <div class="icon">
                      <i class="fas fa-phone"></i>
                    </div>
                    <div class="text">
                      <p>
                        <a href="{{ site.phoneUrl }}">{{ site.phoneText }}</a>
                      </p>
                    </div>
                  </li>
                </ul>
              </div>
              <div class="main-header__top-right">
                <div class="main-header__social">
                  {% include "social.njk" %}
                </div>
                <div class="main-header__btn-box">
                  <a href="/contact/" class="thm-btn main-header__btn"
                    >Book now</a
                  >
                </div>
              </div>
            </div>
          </div>
        </div>

        <nav class="main-menu">
          <div class="main-menu__wrapper">
            <div class="main-menu__wrapper-inner">
              <div class="main-menu__left">
                <div class="main-menu__logo">
                  <a href="/"
                    ><img
                      src="/assets/images/logo/pv-logo-black-3.png"
                      alt=""
                      width="160"
                  /></a>
                </div>
              </div>
              <div class="main-menu__main-menu-box">
                <a href="#" class="mobile-nav__toggler"
                  ><i class="fa fa-bars"></i
                ></a>
                {% include 'nav.njk' %}
              </div>
            </div>
          </div>
        </nav>
      </header>

      <div class="sticky-header sticked-menu main-menu">
        <div class="sticky-header__content"></div>
        <!-- /.sticky-header__content -->
      </div>
      <!-- /.sticky-header -->

      <!--Page Header Start-->
      <section class="page-header">
        <div
          class="page-header__bg"
          style="background-image: url(/assets/images/hero-inner.jpg)"
        ></div>
        <div class="container">
          <div class="page-header__inner">
            <ul class="thm-breadcrumb list-unstyled">
              <li><a href="/">Home</a></li>
              <li><span>/</span></li>
              <li><a href="/treatments/">Treatments</a></li>
              <li><span>/</span></li>
              <li>{{ service.category }}</li>
            </ul>
            <h2>{{ service.title }}</h2>
          </div>
        </div>
      </section>
      <!--Page Header End-->

      <section class="services-details-two">
        <div class="container">
          <div class="row">
            <div class="col-xl-8 col-lg-7 order-lg-1">
              <div class="services-details-two__right">
                <section>
                  <div class="services-details-two__img">
                    <img
                      src="{{ service.thumbnail }}"
                      alt="{{ service.title }}"
                    />
                  </div>
                  <header>
                    <h3 class="services-details-two__title-1">
                      {{ service.title }}
                    </h3>
                  </header>
                  <article>
                    {% set longDescriptionPath = "service-pages/" ~
                    service.longDescription | slug %} {% include
                    longDescriptionPath %} 
                    
                    {% set data = service.faq %} 
                    {% include "faq.njk"%}
                    
                    {% if service.before %}
                      {% include "before-after.njk" %}
                    {% endif %}
                  </article>
                </section>
              </div>
            </div>
            <div class="col-lg-5 col-xl-4 mt-4 mt-lg-0">
              <div class="services-details-two__left">
                <div class="services-details-two__category">
                  {% include "service-detail-nav.njk" %}
                  <!-- <ul class="services-details-two__category-list list-unstyled">
                    <li>
