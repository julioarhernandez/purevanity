<div class="services-details-two__faq">
  <div class="accrodion-grp" data-grp-name="faq-one-accrodion">
    {% for item in data %}
    <div class="accrodion{% if loop.index === 1 %} active{% endif %}">
      <div class="accrodion-title">
        <h4>{{ item.question }}</h4>
      </div>
      <div class="accrodion-content">
        <div class="inner">
          <p>{{ item.answer }}</p>
        </div>
      </div>
    </div>
    {% endfor %}
  </div>
</div>
